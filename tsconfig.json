{
  "compilerOptions": {
    "noEmit": true,
    "sourceMap": false,
    "jsx": "react-jsx",
    "jsxImportSource": "jsx-dom-runtime",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "target": "ESNext",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "lib": [
      "DOM",
      "ES2023"
    ]
  },
  "include": [
    "src"
  ]
}
